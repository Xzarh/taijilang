// use! has been implemented in core.coffee
/.use! #= (filePath, names) -=>
  `{
     exp = parseFile! ^filePath
     $taijiModule = new TaijiModule($taijiModule)
     m = run! ($taijiModule) -> exp
     for name in names then
       if asName then asName = m.name
       else name = m.name
  }